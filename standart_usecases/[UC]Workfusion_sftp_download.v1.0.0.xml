                <template>
                    <content>&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;config xmlns="http://web-harvest.sourceforge.net/schema/1.0/config" scriptlang="groovy"&gt;
    

    &lt;required name="file_path"&gt;&lt;/required&gt;
	&lt;required name="ftp_server"&gt;&lt;/required&gt;
	
  	&lt;var-def name="file_path"&gt;
      &lt;template&gt;{{file_path}}&lt;/template&gt;
	&lt;/var-def&gt;
  	&lt;var-def name="ftp_server"&gt;
      &lt;template&gt;{{ftp_server}}&lt;/template&gt;
	&lt;/var-def&gt;
  	&lt;var-def name="ftp_port"&gt;
      &lt;template&gt;{{ftp_port}}&lt;/template&gt;
	&lt;/var-def&gt;

  	&lt;case&gt;
		&lt;if condition="${'${'+'ftp_port.toString().isEmpty()' + '}'}"&gt;
			&lt;var-def name="ftp_port"&gt;
				&lt;template&gt;21&lt;/template&gt;
			&lt;/var-def&gt;
		&lt;/if&gt;
	&lt;/case&gt;
  
  	 	
    &lt;var-def name="fileContent"&gt;
  		&lt;ftp server="${ftp_server}" port="${ftp_port}"&gt;
			&lt;ftp-get path="${file_path}"&gt;&lt;/ftp-get&gt;
		&lt;/ftp&gt;
  	&lt;/var-def&gt;
       
        
    &lt;export include-original-data="true"&gt;
      &lt;#noparse&gt;
        &lt;single-column name="{{output_column_name}}" value="${fileContent}" /&gt;
      &lt;/#noparse&gt;
    &lt;/export&gt;  

&lt;/config&gt;</content>
